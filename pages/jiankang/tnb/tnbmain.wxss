/* pages/jiankang/tnb/tnbmain.wxss */
.contentview{
  margin: 20px;
  border-bottom: 1px solid royalblue;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.titleTxt{
  color:cadetblue;
  text-align: center;
  font-size: 20pt;
  vertical-align: middle;
  margin-top:10px;
  margin-left: 20px;
  font-weight: bold;
}

.itemImage{
  width: 100%;
  height: 170px;
}

.itemTxt{
  color:cadetblue;
  margin:10px;
}

.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}